num = int(input("Enter a number between 10 and 20:"))

while num > 20 or num < 10:
  if num > 20:
    print("Too high, please try again.")
    num = int(input("Enter a number between 10 and 20:"))
  if num < 10:
    print("Too low, please try again.")
    num = int(input("Enter a number between 10 and 20:"))
else:
  print("Thank you.")
